<h1>Groups#edit</h1>
<p>Find me in app/views/groups/edit.html.erb</p>


<%= form_for @group, url: user_group_path(@current_user.id, @group.id), method: :put do |f| %>
<%= f.text_field :title%>
<%= f.submit :change%>
<%end%>


<%= form_for @invite , :url => invites_path do |f| %>
    <%= f.hidden_field :group_id, :value => @invite.group_id %>
    <%= f.label :email %>
    <%= f.email_field :email %>
    <%= f.submit 'Send' %>
<% end %>